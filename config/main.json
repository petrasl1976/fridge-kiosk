{
  "system": {
    "name": "Fridge Kiosk",
    "theme": "dark",
    "orientation": "portrait",
    "displayTimeout": 60,
    "logLevel": "INFO",
    "fontFamily": "Courier New, monospace"
  },
  "enabledPlugins": [
    "google-photos",
    "google-calendar",
    "google-calendar-summary",
    "sensors",
    "date-time",
    "weather-forecast",
    "discord-channel"
  ],
  "userColors": {
    "PE": "#4d26f0",
    "BU": "#4d26f0",
    "LI": "#018501",
    "MA": "#F44336",
    "LA": "#3e5393",
    "DA": "#607D8B",
    "GI": "#E91E63"
  }
} 